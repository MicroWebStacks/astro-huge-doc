---
import DiagramCode from './DiagramCode.astro'

export interface Props {
    language: string;
    code: string;
    params: object;
    meta: object;
    diagramUid: string | null | undefined;
}

const { language, code, params, meta, diagramUid } = Astro.props as Props;

---
{diagramUid && <DiagramCode uid={diagramUid} code={code} params={params} meta={meta} />}
